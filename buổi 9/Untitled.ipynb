    "import turtle\n",
    "import math\n",
    "i=0\n",
    "a=int(input(\"nh廕計 Ox:\"))\n",
    "b=int(input(\"nh廕計 Oy\"))\n",
    "screen=turtle.Sc\n",
    "pen=turtle.Turtle()\n",
    "pen.goto(0,0)\n",
    "distance=math.sqrt((a**2)+(b**2))\n",
    "while distance<a:\n",
    "    pen.forward(i)\n",
    "    pen.right(10)\n",
    "    i+=0.05\n",
    "    if pen.distance(0,pen)==distance:\n",
    "        break"
   ]
